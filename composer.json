    "description": "Client for parts' prices' services",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5||^7.0"
    },
    "autoload": {
        "psr-4": {
